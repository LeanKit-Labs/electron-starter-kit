{
  "name": "test-electron-compile",
  "version": "0.1.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "jscs": "jscs -x src/*.js src/**/*.js",
    "eslint": "eslint src/**/*.js",
    "pretest": "npm run jscs && npm run eslint",
    "start": "electron ./src/main",
    "clean-cache": "rm -rf ./.cache",
    "compile": "electron-compile ./src -a .",
    "prepackage": "npm run clean-cache && npm run compile",
    "package": "electron-packager . test-electron-compile --platform=darwin --arch=x64 --version=0.37.2 --overwrite",
    "test": "mocha --compilers js:babel-core/register --reporter spec ./spec/*.spec.js --require ./spec/helpers/setup.js --colors"
  },
  "author": "David Neal <david@reverentgeek.com> (http://reverentgeek.com)",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "electron-debug": "^0.5.2",
    "electron-localshortcut": "^0.6.0",
    "font-awesome": "^4.5.0",
    "fs-jetpack": "^0.7.3",
    "jquery": "^2.2.2",
    "lodash": "^4.8.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "chai": "*",
    "electron-compile": "2.1.3",
    "electron-compilers": "2.1.3",
    "electron-packager": "*",
    "electron-prebuilt-compile": "0.37.3",
    "electron-reload": "^0.2.0",
    "eslint": "^2.6.0",
    "eslint-config-leankit": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0"
  }
}
